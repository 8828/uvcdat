
set(ENV{@LIBRARY_PATH@} "@cdat_BINARY_DIR@/Externals/lib:$ENV{@LIBRARY_PATH@}")")
set(ENV{CFLAGS} "@cdat_osx_flags@ -I@cdat_BINARY_DIR@/Externals/include -I@cdat_BINARY_DIR@/Externals/include/freetype2 @ADDITIONAL_CFLAGS@")
set(ENV{CPPFLAGS} "@cdat_osx_flags@ -I@cdat_BINARY_DIR@/Externals/include -I@cdat_BINARY_DIR@/Externals/include/freetype2 @ADDITIONAL_CPPFLAGS@")
set(ENV{CXXFLAGS} "@cdat_osx_flags@ -I@cdat_BINARY_DIR@/Externals/include -I@cdat_BINARY_DIR@/Externals/include/freetype2 @ADDITIONAL_CXXFLAGS@")

set(ENV{LOCNCCONFIG} "@cdat_BINARY_DIR@/Externals/bin/nc-config")

set(ENV{EXTERNALS} "@cdat_BINARY_DIR@/Externals")

execute_process(
  COMMAND @PYTHON_EXECUTABLE@ install.py @qt_flags@
  WORKING_DIRECTORY "${WORKING_DIR}"
  RESULT_VARIABLE res)

if(NOT ${res} EQUAL 0)
  message("Make Errors detected: \n${CDAT_OUT}\n${CDAT_ERR}")
  message(FATAL_ERROR "Error in Python Install")
endif()
message("Make succeeded.")

